# ETL Pipeline Environment Variables
# Copy to .env and fill in values

# Database connection
ETL_DATABASE_URL=postgresql://user:password@localhost:5432/dbname

# API credentials
ETL_API_KEY=your_api_key_here
ETL_API_BASE_URL=https://api.example.com

# Logging
ETL_LOG_LEVEL=INFO
ETL_LOG_FILE=logs/etl.log

# Pipeline settings
ETL_BATCH_SIZE=1000
ETL_RETRY_COUNT=3
